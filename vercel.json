{
  "redirects": [
    {
      "source": "/lander",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/lander/",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "destination": "https://www.washmates.ca/$1",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "washmates.ca"
        }
      ]
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}